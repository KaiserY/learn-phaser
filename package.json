{
  "scripts": {
    "clean": "rimraf app/dist",
    "clean:dist": "rimraf dist/",
    "start": "webpack-dev-server --inline --progress --display-error-details --port 3000",
    "electron": "npm run build:dev && electron ./app",
    "test": "karma start",
    "build:dev": "npm run clean && webpack --progress --profile --colors --display-reasons --display-error-details",
    "build:prod": "npm run clean && webpack --config config/webpack.prod.js --progress --display-error-details --profile --bail",
    "postinstall": "typings install",
    "pack": "npm run clean:dist && npm run build:prod && build",
    "dist": "npm run clean:dist && npm run build:prod && build"
  },
  "build": {
    "download": {
      "mirror": "https://npm.taobao.org/mirrors/electron/",
      "strictSSL": false
    },
    "linux": {
      "target": "rpm",
      "depends": []
    },
    "win": {
      "msi": false
    }
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.2",
    "core-js": "^2.4.0",
    "material-design-icons": "^2.2.3",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.9",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^1.1.1",
    "codelyzer": "0.0.24",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "devtron": "^1.2.1",
    "electron-builder": "^5.10.2",
    "electron-prebuilt": "^1.2.5",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.21.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.1.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.13.0",
    "tslint-loader": "^2.1.5",
    "typescript": "^1.8.10",
    "typings": "^1.3.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"
  }
}
