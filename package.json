{
  "scripts": {
    "rimraf": "rimraf",
    "tslint": "tslint",
    "typings": "typings",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "webdriver-manager": "webdriver-manager",
    "protractor": "protractor",
    "clean": "rimraf app/dist",
    "clean:dist": "rimraf dist/",
    "start": "npm run build:dev && concurrently \"webpack --colors --watch\" \"lite-server\"",
    "electron": "npm run build:dev && electron ./app",
    "test": "karma start",
    "build:dev": "npm run clean && webpack --progress --profile --colors --display-reasons --display-error-details",
    "build:prod": "npm run clean && webpack --config config/webpack.prod.js --progress --display-error-details --profile --bail",
    "postinstall": "typings install",
    "pack": "npm run clean:dist && npm run build:prod && build",
    "dist": "npm run clean:dist && npm run build:prod && build",
    "webdriver:update": "npm run webdriver-manager update",
    "webdriver:start": "npm run webdriver-manager start",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e": "npm run protractor"
  },
  "build": {
    "download": {
      "mirror": "https://npm.taobao.org/mirrors/electron/",
      "strictSSL": false
    },
    "linux": {
      "target": "rpm",
      "depends": []
    },
    "win": {
      "msi": false
    }
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.2",
    "@angular2-material/button": "^2.0.0-alpha.6",
    "@angular2-material/card": "^2.0.0-alpha.6",
    "@angular2-material/checkbox": "^2.0.0-alpha.6",
    "@angular2-material/core": "^2.0.0-alpha.6",
    "@angular2-material/grid-list": "^2.0.0-alpha.6",
    "@angular2-material/input": "^2.0.0-alpha.6",
    "@angular2-material/list": "^2.0.0-alpha.6",
    "@angular2-material/sidenav": "^2.0.0-alpha.6",
    "@angular2-material/tabs": "^2.0.0-alpha.6",
    "@angular2-material/toolbar": "^2.0.0-alpha.6",
    "core-js": "^2.4.0",
    "material-design-icons": "^2.2.3",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.9",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^1.1.1",
    "browser-sync": "^2.13.0",
    "codelyzer": "0.0.24",
    "concurrently": "^2.1.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "devtron": "^1.2.1",
    "electron-builder": "^5.10.5",
    "electron-prebuilt": "^1.2.5",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.1.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "lite-server": "^2.2.2",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "^3.3.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.3",
    "style-loader": "^0.13.1",
    "ts-node": "^0.9.3",
    "tslint": "^3.13.0",
    "tslint-loader": "^2.1.5",
    "typescript": "^1.8.10",
    "typings": "^1.3.1",
    "webpack": "^1.13.1",
    "webpack-merge": "^0.14.0"
  }
}
